[{"ar":" قُلْ يَا أَيُّهَا الْكَافِرُونَ","id":"Katakanlah: \"Hai orang-orang kafir,","nomor":1,"tr":"qul yaa ayyuhaa alkaafiruuna"},{"ar":"لَا أَعْبُدُ مَا تَعْبُدُونَ","id":"Aku tidak akan menyembah apa yang kamu sembah.","nomor":2,"tr":"laa a'budu maa ta'buduuna"},{"ar":"وَلَا أَنْتُمْ عَابِدُونَ مَا أَعْبُدُ","id":"Dan kamu bukan penyembah Tuhan yang aku sembah.","nomor":3,"tr":"walaa antum 'aabiduuna maa a'budu"},{"ar":"وَلَا أَنَا عَابِدٌ مَا عَبَدْتُمْ","id":"Dan aku tidak pernah menjadi penyembah apa yang kamu sembah,","nomor":4,"tr":"walaa anaa 'aabidun maa 'abadtum"},{"ar":"وَلَا أَنْتُمْ عَابِدُونَ مَا أَعْبُدُ","id":"dan kamu tidak pernah (pula) menjadi penyembah Tuhan yang aku sembah.","nomor":5,"tr":"walaa antum 'aabiduuna maa a'budu"},{"ar":"لَكُمْ دِينُكُمْ وَلِيَ دِينِ","id":"Untukmu agamamu, dan untukkulah, agamaku\".","nomor":6,"tr":"lakum diinukum waliya diini"}]